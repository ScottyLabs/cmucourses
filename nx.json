{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "db-generate": {
      "dependsOn": [
        "^db-generate"
      ]
    },
    "dev": {
      "dependsOn": [
        "^db-generate"
      ]
    },
    "build": {
      "dependsOn": [
        "^db-generate"
      ]
    },
    "format": {
      "cache": true
    },
    "lint": {
      "cache": true
    },
    "type-check": {
      "cache": true
    },
    "test": {
      "cache": true
    }
  },
  "defaultBase": "main",
  "plugins": [
    {
      "plugin": "@nx/next/plugin",
      "options": {
        "startTargetName": "next:start",
        "buildTargetName": "next:build",
        "devTargetName": "next:dev",
        "serveStaticTargetName": "serve-static"
      }
    }
  ]
}
